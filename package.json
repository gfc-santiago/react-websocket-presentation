{
  "name": "react-websocket-presentation",
  "description": "Presenter navigated using websockets",
  "workspaces": ["packages/*"],
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  },
  "scripts": {
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "server": "cd ./packages/api && npm run dev",
    "client": "cd ./packages/web && npm run dev"
  }
}
